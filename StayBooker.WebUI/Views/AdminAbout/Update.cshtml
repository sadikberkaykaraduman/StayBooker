@model UpdateAboutDto

@{
    ViewData["Title"] = "Update";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <input type="hidden" asp-for="AboutId" />
    <label>Title 1</label>
    <input type="text" asp-for="AboutTitle1" class="form-control" />
    <br />
    <label>Title 2</label>
    <input type="text" asp-for="AboutTitle2" class="form-control" />
    <br />
    <label>Room</label>
    <input type="text" asp-for="AboutRoomCount" class="form-control" />
    <br />
    <label>Staff</label>
    <input type="text" asp-for="AboutStaffCount" class="form-control" />
    <br />
    <label>Customer</label>
    <input type="text" asp-for="AboutCustomerCount" class="form-control" />
    <br />
    <label>Description</label>
    <textarea type="text" asp-for="AboutContent" class="form-control"></textarea>
    <br />
    <button class="btn btn-primary">Update</button>
</form>