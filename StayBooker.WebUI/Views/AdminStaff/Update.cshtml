@model UpdateStaffDto
@{
    ViewData["Title"] = "Update";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <input type="hidden" asp-for="StaffId" />
    <label>Name</label>
    <input type="text" asp-for="StaffName" class="form-control" />
    <br />
    <label>Title</label>
    <input type="text" asp-for="StaffTitle" class="form-control" />
    <br />
    <label>Social Media 1</label>
    <input type="text" asp-for="StaffSocialMedia1" class="form-control" />
    <br />
    <label>Social Media 2</label>
    <input type="text" asp-for="StaffSocialMedia2" class="form-control" />
    <br />
    <label>Social Media 3</label>
    <input type="text" asp-for="StaffSocialMedia3" class="form-control" />
    <br />
    <button class="btn btn-primary">Update</button>
</form>

