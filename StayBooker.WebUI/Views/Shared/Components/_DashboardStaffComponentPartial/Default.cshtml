@model List<ResultStaffDto>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-lg-3 col-sm-6">
            <div class="card">
                <div class="card-body">
                    <div class="text-center">
                        <img src="@item.StaffImage" class="rounded-circle" alt="">
                        <h5 class="mt-3 mb-1">@item.StaffName</h5>
                        <p class="m-0">@item.StaffTitle</p>
                    </div>
                </div>
            </div>
        </div>
    }
</div>